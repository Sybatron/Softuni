@{
    var nameOfPerson = "";
    var phoneNumber = "";
    try
    {
        nameOfPerson = ViewData["Name"].ToString();
        phoneNumber = ViewData["Phone number"].ToString();
    }
    catch
    {
    }

    var phonebook = new Dictionary<string, string>();
}
<h1>Phonebook</h1>
<table>
    <tr>
        <th style="padding-right:15px">Name</th>
        <th>Number</th>
    </tr>
        <tr>
            <td style="padding-right:15px">@nameOfPerson</td>
            <td>@phoneNumber</td>
        </tr>
</table>
<h1>New contact</h1>
<form>
    Name <input name="name" value="@nameOfPerson" style="margin: 5px 5px 5px 5px;"/>
    <br />
    Number <input name="number" value="@phoneNumber"  style="margin: 5px 5px 5px 5px"/>
    <br />
    <input type="submit" value="Add"  />
</form>